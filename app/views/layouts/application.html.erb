<!DOCTYPE html>
<html>
<head>
  <title>Eventhall</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'bootstrap-select' %>
  <%= stylesheet_link_tag 'bootstrap-select' %>
  <link href='https://fonts.googleapis.com/css?family=Questrial' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Life+Savers:700' rel='stylesheet' type='text/css'>
  <%= stylesheet_link_tag 'variables' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/nav' %>

<div class="container">
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

   <%= yield %>

</div>


</body>
</html>
